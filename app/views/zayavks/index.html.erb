<p id="notice"><%= notice %></p>

<h1>Zayavks</h1>

<table>
  <thead>
    <tr>
      <th>Podr</th>
      <th>Fio</th>
      <th>Telephone</th>
      <th>Dolzh</th>
      <th>Servers</th>
      <th>Infosyst</th>
      <th>Soglos</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @zayavks.each do |zayavk| %>
      <tr>
        <td><%= zayavk.podr %></td>
        <td><%= zayavk.fio %></td>
        <td><%= zayavk.telephone %></td>
        <td><%= zayavk.dolzh %></td>
        <td><%= zayavk.servers %></td>
        <td><%= zayavk.infosyst %></td>
        <td><%= zayavk.soglos %></td>
        <td><%= link_to 'Show', zayavk %></td>
        <td><%= link_to 'Edit', edit_zayavk_path(zayavk) %></td>
        <td><%= link_to 'Destroy', zayavk, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Zayavk', new_zayavk_path %>
